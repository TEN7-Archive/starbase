{
  "name": "bootstrap_build",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "browserslist": [
    "last 1 major version",
    ">= 1%",
    "Chrome >= 45",
    "Firefox >= 38",
    "Edge >= 12",
    "Explorer >= 10",
    "iOS >= 9",
    "Safari >= 9",
    "Android >= 4.4",
    "Opera >= 30"
  ],
  "scripts": {
    "style": "sass bootstrap_overrides.scss | postcss --use autoprefixer -o dist/css/custom-bootstrap.css",
    "style-min": "sass bootstrap_overrides.scss --style compressed | postcss --use autoprefixer -o dist/css/custom-bootstrap.min.css",
    "copy-js": "cpx \"node_modules/bootstrap/dist/js/*.js\" dist/js",
    "all": "npm-run-all --parallel style style-min copy-js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^8.2.0",
    "bootstrap": "^4.1.0",
    "cpx": "^1.5.0",
    "npm-run-all": "^4.1.2",
    "postcss-cli": "^5.0.0"
  }
}
